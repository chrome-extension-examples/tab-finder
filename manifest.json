{
  "name": "Tab Finder",
  "version": "1.0",
  "description": "Tab Finder",
  "permissions": ["tabs"],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "default_title": "Marcus Chiu",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "commands": {
    "open-confluence-marcuschiu-com": {
      "suggested_key": { "default": "Alt+Z" },
      "description": "open confluence.marcuschiu.com"
    }
  },
  "manifest_version": 2
}
